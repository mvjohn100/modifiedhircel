
      <!-- Begin page content -->
      <div class="container">


	      <div class="container-fluid">
				<div class="row-fluid">
				<div class="span2">
				<!--Sidebar content-->
					
					<%= render :partial => "settings/left_nav"%>
					
				<!--Body content-->
						<div class = "container-header"><p>Card Credit Info</p></div>
					    <form class="form-horizontal">
							<div class="control-group">
								<label class="control-label" for="inputCardHoderName">Card holder name:</label>
								<div class="controls">
									<input type="text" id="inputCardHolderName" placeholder="Card holder name">
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="inputCardType">Card type:</label>
								<div class="controls">
									   <select class="selectpicker">
											<optgroup label="Pendding">
											<option>Type1</option>
											<option>Type2</option>
											<option>Type3</option>
											</optgroup>
											<optgroup label="Approve">
											<option>Type1</option>
											<option>Type2</option>
											<option>Type3</option>
											</optgroup>
										</select>
									 
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="inputCardNumber">Card number:</label>
								<div class="controls">
									<input type="text" id="inputCardNumber" placeholder="Card number">
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="inputState">State:</label>
								<div class="controls">
									<input type="text" id="inputState" placeholder="State">
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="inputCardType">Card type:</label>
								<div class="controls">
									 <ul class="date-option">
										<li>
										   <select class="selectpicker">
												<optgroup label="Day">
												<option>Type1</option>
												<option>Type2</option>
												<option>Type3</option>
												</optgroup>
												
											</select>
										</li>
										<li>
										    <select class="selectpicker">
												<optgroup label="Month">
												<option>Type1</option>
												<option>Type2</option>
												<option>Type3</option>
												</optgroup>
												
											</select>

										</li>
										<li>
										   <select class="selectpicker">
												<optgroup label="Year">
												<option>Type1</option>
												<option>Type2</option>
												<option>Type3</option>
												</optgroup>
												
											</select>

										</li>
									</ul>
									</div>
								</div>
							<div class="control-group">
								<label class="control-label" for="inputSecurityCode">Security Code:</label>
								<div class="controls">
									<input type="text" id="inputSecurityCode" placeholder="Security Code">
								</div>
							</div>
							<div class="control-group">
								<div class="controls">
								
								<button type="submit" class="btn btn-primary update">Update</button>
								</div>
							</div>
						</form>

				</div>
				</div>
		  </div>

      <div id="push"></div>
    </div>



   